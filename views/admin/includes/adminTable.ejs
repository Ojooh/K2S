<div class="row listn <%= tab.row_list%>">
    <div class="col-12">
        <div class="card card-stats mt-5">
            <div class="card-table-title bg-success">
                <h4 class="card-title ">Administrators Table</h4>
                <p class="card-description"> List of administrators for the Kids To school Foundation.</p>

            </div>
            <div class="card-body mt-5">
                <% if (admins.length> 0) {%>
                    <div class="table-responsive">
                        <table class="table table-hover table-striped">
                            <thead>
                                <tr>
                                    <th scope="col">S/N</th>
                                    <th scope="col">Name</th>
                                    <th scope="col">Title</th>
                                    <th scope="col">Date Created</th>
                                    <th scope="col">Last Login</th>
                                    <th scope="col">Status</th>
                                    <th scope="col">Action</th>
                                </tr>
                            </thead>
                        <% if (admins.length> 1) { %>
                            <tbody>
                            <% for (var i=0; i < admins.length; i++) { %>
                                <tr>

                                    <th scope="row">
                                        <%= i + 1; %>
                                    </th>
                                    <td>
                                        <%= admins[i].fname + " " + admins[i].lname %>
                                    </td>
                                    <td>
                                        <%= admins[i].title %>
                                    </td>
                                    <td class="pretty-date">
                                        <%= admins[i].date_created %>
                                    </td>
                                    <td class="pretty-date">
                                        <%= admins[i].last_login %>
                                    </td>
                                <% if (admins[i].id !=user.id) { %>
                                    <td>
                                    <% if (admins[i].is_active) { %>
                                        <div class="custom-control custom-switch">
                                            <input type="checkbox" class="custom-control-input" id="customSwitch-<%= admins[i].id %>" value="False"
                                                checked>
                                            <label class="custom-control-label" for="customSwitch-<%= admins[i].id %>"> Active</label>
                                        </div>
                                    <% } else { %>
                                        <div class="custom-control custom-switch">
                                            <input type="checkbox" class="custom-control-input" id="customSwitch-<%= admins[i].id %>"
                                                value="True">
                                            <label class="custom-control-label" for="customSwitch-<%= admins[i].id %>"> In-Active</label>
                                        </div>
                                    <% } %>
                                    </td>
                                <% } else { %>
                                    <td></td>
                                <% } %>
                                    <td class="td-actions">
                                        <a href="#" class="profile text-info mr-4" data-id="<%= admins[i].id %>" data-url="/admin/User/get_profile"
                                            data-type="display-modal" data-toggle="tooltip" data-placement="top" title="Administrator Details">
                                            <i class="fas fa-info-circle"></i>
                                        </a>
                                        <a href="#" class="edit-admin text-primary mr-4" data-id="<%= admins[i].id %>" data-url="/admin/User/get_profile"
                                            data-type="edit-modal" data-toggle="tooltip" data-placement="top" title="Edit Administrator">
                                            <i class="fas fa-pencil-alt"></i>
                                        </a>
                                        <% if (admins[i].id !=user.id) { %>
                                            <a href="#" class="delete-admin text-danger" data-id="<%= admins[i].id %>" data-url="/admin/User/delete_profile"
                                                data-toggle="tooltip" data-placement="top" title="Remove Administrator">
                                                <i class="fas fa-times"></i>
                                            </a>
                                            <% } %>
                                    
                                    </td>

                                </tr>
                            <% } %>
                            </tbody>
                        <% } else if (admins.length == 1) { %>
                            <tbody>
                                <% for (var i=0; i < admins[0].length; i++) { %>
                                    <tr>
                                    
                                        <th scope="row">
                                            <%= i + 1; %>
                                        </th>
                                        <td>
                                            <%= admins[0][i].fname + " " + admins[0][i].lname %>
                                        </td>
                                        <td>
                                            <%= admins[0][i].title %>
                                        </td>
                                        <td class="pretty-date">
                                            <%= admins[0][i].date_created %>
                                        </td>
                                        <td class="pretty-date">
                                            <%= admins[0][i].last_login %>
                                        </td>
                                        <% if (admins[0][i].id !=user.id) { %>
                                            <td>
                                                <% if (admins[0][i].is_active) { %>
                                                    <div class="custom-control custom-switch">
                                                        <input type="checkbox" class="custom-control-input" id="customSwitch-<%= admins[0][i].id %>"
                                                            value="False" checked>
                                                        <label class="custom-control-label" for="customSwitch-<%= admins[0][i].id %>"> Active</label>
                                                    </div>
                                                    <% } else { %>
                                                        <div class="custom-control custom-switch">
                                                            <input type="checkbox" class="custom-control-input" id="customSwitch-<%= admins[0][i].id %>"
                                                                value="True">
                                                            <label class="custom-control-label" for="customSwitch-<%= admins[0][i].id %>"> In-Active</label>
                                                        </div>
                                                        <% } %>
                                            </td>
                                            <% } else { %>
                                                <td></td>
                                                <% } %>
                                                    <td class="td-actions">
                                                        <a href="#" class="profile text-info mr-4" data-id="<%= admins[0][i].id %>"
                                                            data-url="/admin/User/get_profile" data-type="display-modal" data-toggle="tooltip"
                                                            data-placement="top" title="Administrator Details">
                                                            <i class="fas fa-info-circle"></i>
                                                        </a>
                                                        <a href="#" class="edit-admin text-primary mr-4" data-id="<%= admins[0][i].id %>"
                                                            data-url="/admin/User/get_profile" data-type="edit-modal" data-toggle="tooltip"
                                                            data-placement="top" title="Edit Administrator">
                                                            <i class="fas fa-pencil-alt"></i>
                                                        </a>
                                                        <% if (admins[0][i].id !=user.id) { %>
                                                            <a href="#" class="delete-admin text-danger" data-id="<%= admins[0][i].id %>"
                                                                data-url="/admin/User/delete_profile" data-toggle="tooltip" data-placement="top"
                                                                title="Remove Administrator">
                                                                <i class="fas fa-times"></i>
                                                            </a>
                                                            <% } %>
                                    
                                                    </td>
                                    
                                    </tr>
                                    <% } %>
                            </tbody>
                        <% } %>

                        </table>
                    </div>
                <% } else { %>
                    <div class="my-5">
                        <h6 class="text-center text-danger">No Administrators Available</h6>
                    </div>
                <% } %>
            </div>
        </div>
    </div>
</div>
<!-- Kids Table End -->