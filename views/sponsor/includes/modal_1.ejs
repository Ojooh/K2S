<div class="overlay deactivated">
    <div class="close-sidebar">
        <i class="fas fa-times"></i>
    </div>
</div>
<!-- Over Lay End -->
<!-- The Modal -->
<!-- Kid Form Modal -->
<div class="modal fade bd-example-modal-xl" id="viewKidModal" data-backdrop="static" tabindex="-1" role="dialog"
    aria-labelledby="viewKidModalCenterTitle" aria-hidden="true">
    <div class="close-modal">
        <i class="fas fa-times"></i>
    </div>

    <div class="modal-dialog modal-dialog-centered modal-xl" role="document">
        <div class="modal-content card-modal-box mt-5">
            <div class="card-modal-header bg-success">
                <h4 class="card-modal-title "></h4>
                <p class="card-modal-description"></p>
    
            </div>
    
            <div class="modal-body card-modal-body card-modal-scrollable mt-5">
                <div class="container-fluid">
                    <div class="row mt-5 mb-4">
                        <div class="col-lg-4">
                            <div class="kid mt-md-5" >
                                <img src="" class="avartart img-fluid">        
                            </div>
                        </div>
                        <div class="col-lg-8 mt-md-4">
                            <ul class="nav nav-tabs" id="myTab" role="tablist">
                                <li class="nav-item" role="presentation">
                                    <a class="nav-link active" id="home-tab" data-toggle="tab" href="#bio" role="tab" aria-controls="bio"
                                        aria-selected="true">Bio</a>
                                </li>
                                <li class="nav-item" role="presentation">
                                    <a class="nav-link" id="profile-tab" data-toggle="tab" href="#SchoolInfo" role="tab" aria-controls="SchoolInfo"
                                        aria-selected="false">School</a>
                                </li>
                                <li class="nav-item" role="presentation">
                                    <a class="nav-link" id="contact-tab" data-toggle="tab" href="#parent" role="tab" aria-controls="parent"
                                        aria-selected="false">Parent</a>
                                </li>
                                <li class="nav-item" role="presentation">
                                    <a class="nav-link" id="contact-tab" data-toggle="tab" href="#profile" role="tab" aria-controls="profile"
                                        aria-selected="false">Profile</a>
                                </li>
                                <li class="nav-item" role="presentation">
                                    <a class="nav-link" id="expense-tab" data-toggle="tab" href="#expense" role="tab" aria-controls="expense"
                                        aria-selected="false">Expenses</a>
                                </li>
                            </ul>
                            <div class="tab-content my-2 p-md-4" id="myTabContent">
                                <div class="tab-pane fade show active" id="bio" role="tabpanel" aria-labelledby="home-tab">
                                    <div class="container-fluid">
                                        <div class="row">
                                            <div class="col-md-12 info">
                                                <div class="data ml-3">
                            
                                                </div>
                            
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="tab-pane fade" id="SchoolInfo" role="tabpanel" aria-labelledby="schoolInfo-tab">
                                    <div class="container">
                                        <div class="row">
                                            <div class="col-md-12 s-info">
                                                <div class="data ml-3">
                            
                                                </div>
                            
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                
                                <div class="tab-pane fade" id="parent" role="tabpanel" aria-labelledby="parent-tab">
                                    <div class="container">
                                        <div class="row">
                                            <div class="col-md-12 p-info">
                                                <div class="data ml-3">
                            
                                                </div>
                            
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            
                                <div class="tab-pane fade" id="profile" role="tabpanel" aria-labelledby="profile-tab">
                                    <div class="container">
                                        <div class="row">
                                            <div class="col-md-12 pro-info">
                                                <div class="data ml-3">
                            
                                                </div>
                            
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="tab-pane fade" id="expense" role="tabpanel" aria-labelledby="expense-tab">
                                    <div class="container">
                                        <div class="row">
                                            <div class="col-md-12 exp-info">
                                                <div class="data ml-3">
                                
                                                </div>
                                
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                
            </div>
            <div class="modal-footer">
                <div class="container">
                    <div class="row">
                        <div class="col-md-6">
                            
                            <button type="button" class="btn btn-outline-kts rounded-pill w-100 py-2" id="donateKid" data-id="" data-url="" data-name="">
                                <i class="far fa-handshake"></i>
                               Donate
                            </button>
                        </div>
                        <div class="col-md-6">
                            <button type="button" class="btn btn-kts rounded-pill w-100 py-2" id="adoptKid" data-id=""
                                data-name="" data-sname="<%= user.fname %>" data-url="/sponsor/Kid/adopt">
                                Adopt
                                <i class="fas fa-donate"></i>
                            </button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

